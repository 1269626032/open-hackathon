[uwsgi]

base = <%= node['open-hackathon']['api']['src_dir'] %>

app = run

module = %(app)

pythonpath = <%= node['open-hackathon']['api']['src_dir'] %>

enable-threads = true

http = 0.0.0.0:<%= node['open-hackathon']['api']['port'] %>

chmod-socket    = 666

callable = app

logto = <%= node['open-hackathon']['uwsgi']['log_file'] %>
