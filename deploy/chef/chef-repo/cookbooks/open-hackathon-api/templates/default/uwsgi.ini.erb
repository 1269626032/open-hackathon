[uwsgi]

base = <%= node['open-hackathon-api']['uwsgi']['base'] %>

app = run

module = %(app)

pythonpath = <%= node['open-hackathon-api']['uwsgi']['pythonpath'] %>

enable-threads = true

http = 0.0.0.0:<%= node['open-hackathon-api']['uwsgi']['http-port'] %>

chmod-socket    = 666

callable = app

logto = <%= node['open-hackathon-api']['uwsgi']['logto'] %>
