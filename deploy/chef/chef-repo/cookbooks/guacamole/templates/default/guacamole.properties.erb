# Hostname and port of guacamole proxy
guacd-hostname: <%= node['guacamole']['guacd-hostname'] %>
guacd-port:     <%= node['guacamole']['guacd-port'] %>

lib-directory: <%= node['guacamole']['lib-directory'] %>

# Auth provider class (authenticates user/pass combination, needed if using the provided login screen)
auth-provider: <%= node['guacamole']['auth-provider'] %>
auth-request-url: <%= node['guacamole']['auth-request-url'] %>
